            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rodjeep.goodhealthgoodlife.Home" />
        </activity>
    </application>

</manifest>